{
  "name": "caviar",
  "version": "1.0.0",
  "license": "MIT",
  "types": "./types/index.ts",
  "files": [
    "abi",
    "artifacts",
    "contracts",
    "deployments",
    "exports",
    "types"
  ],
  "author": "",
  "scripts": {
    "build": "hardhat compile",
    "console": "hardhat console",
    "export": "hardhat export --export-all exports/deployments.json",
    "test": "hardhat test --verbose",
    "test:coverage": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048\" hardhat coverage",
    "test:gas": "cross-env REPORT_GAS=true yarn test",
    "prettier": "prettier --write test/**/*.{ts} && prettier --write contracts/**/*.sol",
    "lint": "yarn prettier && solhint -c .solhint.json contracts/**/*.sol",
    "prepublishOnly": "yarn run build && node scripts/prepublish.js",
    "gelato:test:deploy:fee-manager": "hardhat run ./gelato/deploy/create-fee-manager.js --network mumbai",
    "gelato:deploy:fee-manager": "hardhat run ./gelato/deploy/create-fee-manager.js --network polygon"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.0",
    "@openzeppelin/hardhat-upgrades": "^1.27.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@sushiswap/sdk": "^4.1.1",
    "@tenderly/hardhat-tenderly": "^1.0.0",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-deploy": "^0.11.34",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-preprocessor": "^0.1.5",
    "hardhat-spdx-license-identifier": "^2.1.0",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.7.13",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.2",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@gelatonetwork/automate-sdk": "^2.8.0-beta",
    "@gelatonetwork/web3-functions-sdk": "^2.0.7"
  }
}
